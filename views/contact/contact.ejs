            <li class="nav-item active" style="animation: yourAnimation 1s 1 1s linear;">
              <a class="nav-link" href="contact/contact">Contact</a>
            </li>  
            <li class="nav-item" style="animation: yourAnimation 1s 1 1s linear;">
              <a class="nav-link" href="/user/login" target="_blank"><i class="fa fa-user-circle-o"></i></a>
            </li>   
          </ul>
        </div>  
    </nav>
      
    <div class="hero-image">
        <div class="hero-text">
            <h1 class="ht" style="animation: yourAnimation 1s 1 1s linear;">Contact</h1>
        </div>
    </div>
    <br><br><br><br><br><br>
    
    <div class="container">
      <div><h3>Contact Form</h3></div>
      <div class="contact">
        <div class="row">
          <% if(message.length>0){ %>
            <div class="message"><p><%= message %></p></div>
          <% } %>  
            <div class="form col-md-12">
              <form action="/contact" method="POST">
                <label for="name">Name :</label>
                <input type="text" id="name" name="name" placeholder="Name" required>
                <label for="msg">Message :</label>
                <textarea rows="10" id="msg" cols="10" name="msg" placeholder="Message" required></textarea>
                <button type="submit">SUBMIT</button>
              </form>
          </div>
        </div>
      </div>
      <br><br><br>
      
      <div>
        <h3>OUR HEAD OFFICE</h3>
      </div>
      <div class="info">
        <p style="text-align: center;">
          WILSON ARCHITECTS, 564 BOUNDARY ST, SPRING HILL, QLD, 4000, AUSTRALIA<br>CONTACT :07 3831 2755<br>WA@WILSONARCHITECTS.COM.AU
        </p>
      </div>
      <br>
      <div id="map"></div>
    </div>
    <br><br><br>
    <div class="feet">
        <br><br>
        <p style="text-align: center; color: white; ">
            WILSON ARCHITECTS, 564 BOUNDARY ST, SPRING HILL, QLD, 4000, AUSTRALIA<br>CONTACT :07 3831 2755<br>WA@WILSONARCHITECTS.COM.AU
        </p>
        <div class="sma" style="margin-left: 45%;">
            <div><img class="sg" src="./images/instagram-sketched.svg" alt=""><img src="./images/facebook.svg" alt="" style="height: 48px;"><img class="sg" src="./images/twitter.svg" alt=""><img class="sg" src="./images/linkedin.svg" alt=""></div>
        </div>
    </div>
    <script src="./script/contact.js"></script>
    <script defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCzT1r1sOloPN68_eMEcAq6QCyqTTulRPo&callback=initMap">
    </script>
</body>
</html>